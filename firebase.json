{
  "hosting": {
    "site": "pcps",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "pcps-physics",
          "region": "asia-east1"
        }
      }
    ]
  }
}
